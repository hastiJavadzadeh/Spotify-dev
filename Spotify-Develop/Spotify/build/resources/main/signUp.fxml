<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="754.0" prefWidth="657.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignUpController">
    <children>
        <Label layoutX="56.0" layoutY="48.0" text="Sign up for free to start listening.">
            <font>
                <Font name="Arial Black" size="30.0" />
            </font></Label>
        <Button fx:id="buttonLogin" layoutX="356.0" layoutY="709.0" mnemonicParsing="false" onAction="#switchToLogin" style="-fx-background-color: transparent;" text="Log in" textFill="#1fd655" underline="true" />
        <TextField fx:id="tfEmail" layoutX="88.0" layoutY="152.0" prefHeight="31.0" prefWidth="502.0" promptText="Enter your email." />
        <TextField fx:id="tfUsername" layoutX="88.0" layoutY="327.0" prefHeight="31.0" prefWidth="502.0" promptText="Enter a profile name." />
        <TextField fx:id="tfPassword" layoutX="88.0" layoutY="242.0" prefHeight="31.0" prefWidth="502.0" promptText="Create a password." />
        <Button fx:id="buttonSignup" layoutX="228.0" layoutY="616.0" mnemonicParsing="false" onAction="#switchToWelcome" prefHeight="80.0" prefWidth="165.0" style="-fx-background-color: #1fd655;" text="Sign up">
            <font>
                <Font name="Arial" size="21.0" />
            </font>
        </Button>
        <Label layoutX="90.0" layoutY="118.0" text="what's your email?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="91.0" layoutY="204.0" text="create a password">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="88.0" layoutY="295.0" text="what should we call you?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="88.0" layoutY="362.0" text="This appears on your profile.">
            <font>
                <Font name="B Arash" size="16.0" />
            </font>
        </Label>
        <Label layoutX="187.0" layoutY="712.0" text="Have an account?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="90.0" layoutY="414.0" text="what's your date of birth?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="tfBirthday" layoutX="88.0" layoutY="449.0" prefHeight="57.0" prefWidth="502.0" promptText="yyyy-mm-dd" />
        <Label layoutX="91.0" layoutY="526.0" text="Choose a profile picture">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="tfPicture" layoutX="88.0" layoutY="558.0" prefHeight="31.0" prefWidth="502.0" promptText="Enter the path of your picture." />
    </children>
</AnchorPane>
