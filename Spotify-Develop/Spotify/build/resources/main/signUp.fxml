<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="736.0" prefWidth="796.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignUpController">
    <children>
        <Label layoutX="133.0" layoutY="117.0" text="Sign up for free to start listening.">
            <font>
                <Font name="Arial Black" size="30.0" />
            </font></Label>
        <Button fx:id="buttonLogin" layoutX="434.0" layoutY="784.0" mnemonicParsing="false" onAction="#switchToLogin" style="-fx-background-color: transparent;" text="Log in" textFill="#1fd655" underline="true" />
        <TextField fx:id="tfEmail" layoutX="166.0" layoutY="227.0" prefHeight="31.0" prefWidth="502.0" promptText="Enter your email." />
        <TextField fx:id="tfUsername" layoutX="166.0" layoutY="402.0" prefHeight="31.0" prefWidth="502.0" promptText="Enter a profile name." />
        <TextField fx:id="tfPassword" layoutX="166.0" layoutY="317.0" prefHeight="31.0" prefWidth="502.0" promptText="Create a password." />
        <Button fx:id="buttonSignup" layoutX="306.0" layoutY="691.0" mnemonicParsing="false" onAction="#switchToWelcome" prefHeight="80.0" prefWidth="165.0" style="-fx-background-color: #1fd655;" text="Sign up">
            <font>
                <Font name="Arial" size="21.0" />
            </font>
        </Button>
        <Label layoutX="168.0" layoutY="193.0" text="what's your email?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="169.0" layoutY="279.0" text="create a password">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="166.0" layoutY="370.0" text="what should we call you?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="166.0" layoutY="437.0" text="This appears on your profile.">
            <font>
                <Font name="B Arash" size="16.0" />
            </font>
        </Label>
        <Label layoutX="265.0" layoutY="787.0" text="Have an account?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <Label layoutX="168.0" layoutY="489.0" text="what's your date of birth?">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="tfBirthday" layoutX="166.0" layoutY="524.0" prefHeight="57.0" prefWidth="502.0" promptText="yyyy-mm-dd" />
        <Label layoutX="169.0" layoutY="601.0" text="Choose a profile picture">
            <font>
                <Font name="Arial Black" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="tfPicture" layoutX="166.0" layoutY="633.0" prefHeight="31.0" prefWidth="502.0" promptText="Enter the path of your picture." />
    </children>
</AnchorPane>

