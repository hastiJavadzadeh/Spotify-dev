    <children>
        <Button fx:id="searchButton" layoutX="458.0" layoutY="96.0" mnemonicParsing="false" onAction="#switchToResult" style="-fx-background-color: transparent;" text="search" textFill="#1fd655" underline="true">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
