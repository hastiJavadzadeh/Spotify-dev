            <font>
                <Font name="Arial" size="45.0" />
            </font>
        </Label>
        <ListView fx:id="songsList" layoutX="33.0" layoutY="156.0" prefHeight="440.0" prefWidth="501.0" />
        <TextField fx:id="tfGenre" layoutX="33.0" layoutY="102.0" prefHeight="31.0" prefWidth="415.0" promptText="Enter genre" />
    </children>
</AnchorPane>

